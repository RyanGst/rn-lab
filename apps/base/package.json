		"babel-jest": "^29.7.0",
		"dotenv": "^16.4.5",
		"jest": "^29.7.0",
		"jest-expo": "^51.0.4",
		"supabase": "^1.191.3",
		"ts-node": "^10.9.2",
		"typescript": "~5.3.3"
